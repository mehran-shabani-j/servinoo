{
  "buildCommand": "pnpm run build",
  "devCommand": "pnpm run dev",
  "installCommand": "corepack enable && corepack prepare pnpm@9 --activate && pnpm install --frozen-lockfile",
  "framework": "nextjs",
  "regions": ["fra1"],
  "functions": {
    "app/**/*.ts": {
      "maxDuration": 30
    },
    "app/**/*.tsx": {
      "maxDuration": 30
    }
  }
}
